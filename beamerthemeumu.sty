\mode<presentation>

% Requirement
\RequirePackage{tikz}
\RequirePackage{umucolors}
\selectcolormodel{RGB}

% Settings
\useinnertheme{umu}
\useoutertheme{umu}
\usecolortheme{umu}
\setbeamertemplate{caption}[numbered]
\setbeamertemplate{navigation symbols}{}
\setbeamertemplate{blocks}[rounded][shadow=true]
% Set the color of the blocks title
\setbeamercolor*{block title}{fg=white,bg=umublue}
% Set the color of the blocks body
\setbeamercolor*{block body}{bg=umugrey,fg=black}
\setbeamerfont{block body}{size=\large}
\setbeamerfont{block title}{size=\Large,series=\bfseries}


\setbeamertemplate{itemize items}[ball]
